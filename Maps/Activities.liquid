{
  "date_debut": "{{ date_debut }}",
  "date_debut_parl": "{{ date_debut_parl }}",
  "date_fin": "{{ date_fin }}",
  "data": [
    {% for week in (1..54) %}
      {% capture weekAsString %}{{ week }}{% endcapture %}
      {
        "numero_de_semaine": {{ week }},
        "presences_commission": {{ n_presences.commission[weekAsString] | round | default: 0 }},
        "presences_hemicycle": {{ n_presences.hemicycle[weekAsString] | round | default: 0 }},
        "participations_commission": {{ n_participations.hemicycle[weekAsString] | round | default: 0 }},
        "participations_hemicycle": {{ n_participations.hemicycle[weekAsString] | round | default: 0 }},
        "questions": {{ n_questions[weekAsString] | round | default: 0 }},
        "vacances": {{ vacances[weekAsString] | round | default: 0 }}
      }{% if week != 54 %},{% endif %}
    {% endfor %}
  ]
}
