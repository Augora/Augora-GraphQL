{
  "date_debut": "{{ date_debut }}",
  "date_debut_parl": "{{ date_debut_parl }}",
  "date_fin": "{{ date_fin }}",
  "data": [
    {% for week in (1..54) %}
      {% capture weekAsString %}{{ week }}{% endcapture  %}
      {
        "weekNumber": {{ week }},
        "presencesCommission": {{ n_presences.commission[weekAsString] }},
        "presencesHemicycle": {{ n_presences.hemicycle[weekAsString] }},
        "participationsCommission": {{ n_participations.hemicycle[weekAsString] }},
        "participationsHemicycle": {{ n_participations.hemicycle[weekAsString] }},
        "questions": {{ n_questions[weekAsString] }},
        "vacances": {{ vacances[weekAsString] }}
      }{% if week != 54 %},{% endif %}
    {% endfor %}
  ]
}

